<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.littlejie.adapter">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".array.SimplestArrayAdapterActivity" />
        <activity android:name=".array.HardestArrayAdapterActivity" />
        <activity android:name=".array.MiddleArrayAdapterActivity" />
        <activity android:name=".array.ListActionActivity" />
        <activity android:name=".cursor.MyCursorAdapterActivity" />
        <activity android:name=".cursor.MyResourceCursorAdapterActivity" />
        <activity android:name=".cursor.MySimpleCursorAdapterActivity" />
        <activity android:name=".array.ArrayAdapterActivity" />
        <activity android:name=".cursor.CursorAdapterActivity"></activity>
    </application>

</manifest>
